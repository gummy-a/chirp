{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next: Server",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Next: Browser",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    }
  ],
  "compounds": [
    {
      "name": "Next: Fullstack",
      "configurations": ["Next: Server", "Next: Browser"]
    }
  ]
}